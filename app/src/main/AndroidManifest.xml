        <activity android:name=".View.ADMPackage.RegisterRequestActivity" />
        <activity android:name=".View.ADMPackage.AdminMainActivity" />
        <activity android:name=".View.USERPackage.BookActivity" />
        <activity android:name=".View.MAINPackage.RegisterStudentActivity" />
        <activity android:name=".View.MAINPackage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.USERPackage.SeeMenuActivity" />
    </application>

</manifest>