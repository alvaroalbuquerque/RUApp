        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.ADMPackage.RegisterMenuPackage.InsertMenuActivity"></activity>
        <activity android:name=".View.ADMPackage.ReportActivity" />
